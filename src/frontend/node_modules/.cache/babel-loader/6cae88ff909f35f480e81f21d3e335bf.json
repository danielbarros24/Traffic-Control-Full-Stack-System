\n              <template #actions>\n                <v-icon\n                  class=\"mr-1\"\n                  small\n                >\n                  mdi-clock-outline\n                </v-icon>\n\n                <span\n                  class=\"text-caption grey--text font-weight-light\"\n                  v-text=\"chart.time\"\n                />\n              </template>\n            </material-chart-card>\n          </v-col>\n        </v-row>\n      </v-col>\n\n      <v-col\n        v-for=\"({ actionIcon, actionText, ...attrs }, i) in stats\"\n        :key=\"i\"\n        cols=\"12\"\n        md=\"6\"\n        lg=\"3\"\n      >\n        <material-stat-card v-bind=\"attrs\">\n          <template #actions>\n            <v-icon\n              class=\"mr-2\"\n              small\n              v-text=\"actionIcon\"\n            />\n            <div class=\"text-truncate\">\n              {{ actionText }}\n            </div>\n          </template>\n        </material-stat-card>\n      </v-col>\n\n      <v-col\n        cols=\"12\"\n        md=\"6\"\n      >\n        <material-card\n          color=\"orange\"\n          full-header\n        >\n          <template #heading>\n            <div class=\"pa-8 white--text\">\n              <div class=\"text-h4 font-weight-light\">\n                Employees Stats\n              </div>\n              <div class=\"text-caption\">\n                New employees on 15th September, 2016\n              </div>\n            </div>\n          </template>\n          <v-card-text>\n            <v-data-table\n              :headers=\"headers\"\n              :items=\"items\"\n            />\n          </v-card-text>\n        </material-card>\n      </v-col>\n\n      <v-col\n        cols=\"12\"\n        md=\"6\"\n      >\n        <material-card\n          color=\"accent\"\n          full-header\n        >\n          <template #heading>\n            <v-tabs\n              v-model=\"tabs\"\n              background-color=\"transparent\"\n              slider-color=\"white\"\n              class=\"pa-8\"\n            >\n              <span\n                class=\"subheading font-weight-light mx-3\"\n                style=\"align-self: center\"\n              >Tasks:</span>\n              <v-tab class=\"mr-3\">\n                <v-icon class=\"mr-2\">\n                  mdi-bug\n                </v-icon>\n                Bugs\n              </v-tab>\n              <v-tab class=\"mr-3\">\n                <v-icon class=\"mr-2\">\n                  mdi-code-tags\n                </v-icon>\n                Website\n              </v-tab>\n              <v-tab>\n                <v-icon class=\"mr-2\">\n                  mdi-cloud\n                </v-icon>\n                Server\n              </v-tab>\n            </v-tabs>\n          </template>\n          <v-tabs-items\n            v-model=\"tabs\"\n            background-color=\"transparent\"\n          >\n            <v-tab-item\n              v-for=\"n in 3\"\n              :key=\"n\"\n            >\n              <v-card-text>\n                <template v-for=\"(task, i) in tasks[tabs]\">\n                  <v-row\n                    :key=\"i\"\n                    align=\"center\"\n                    class=\"flex-nowrap\"\n                  >\n                    <v-col cols=\"1\">\n                      <v-list-item-action>\n                        <v-simple-checkbox\n                          v-model=\"task.value\"\n                          color=\"secondary\"\n                        />\n                      </v-list-item-action>\n                    </v-col>\n\n                    <v-col\n                      class=\"font-weight-light\"\n                      cols=\"8\"\n                      v-text=\"task.text\"\n                    />\n\n                    <v-col\n                      cols=\"auto\"\n                      class=\"text-right\"\n                    >\n                      <v-icon class=\"mx-1\">\n                        mdi-pencil\n                      </v-icon>\n\n                      <v-icon\n                        class=\"mx-1\"\n                        color=\"error\"\n                      >\n                        mdi-close\n                      </v-icon>\n                    </v-col>\n                  </v-row>\n                </template>\n              </v-card-text>\n            </v-tab-item>\n          </v-tabs-items>\n        </material-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n</template>\n\n<script>\n\n  export default {\n    name: 'DashboardView',\n  }\n</script>\n"]},"metadata":{},"sourceType":"module"}